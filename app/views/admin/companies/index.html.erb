
  <!-- Breadcrumbs-->
  <ol class="breadcrumb">
    <li class="breadcrumb-item">
      <%= link_to 'Dapps', admin_companies_path %>
    </li>
    <li class="breadcrumb-item active">Dapps</li>
  </ol>
  <div class="table-responsive">
    <table class="table table-bordered" width="100%" cellspacing="0">
      <thead>
        <tr>
          <th>Name</th>
          <th>Website</th>
          <th>White paper</th>
          <th>Age</th>
          <th>Start date</th>
          <th>Published</th>
        </tr>
      </thead>
      <tfoot>
        <tr>
          <th>Name</th>
          <th>Position</th>
          <th>Office</th>
          <th>Age</th>
          <th>Start date</th>
          <th>Published</th>
        </tr>
      </tfoot>
      <tbody>
				<% @companies.each do |company|%>
        <tr>
          <td><%= link_to company.name, admin_company_path(company) %></td>
          <td><%= link_to company.website, company.website, target: "_blank" %></td>
          <td><%= link_to company.white_paper, company.white_paper, target: "_blank" %></td>
          <td></td>
          <td></td>
          <td><%= company.published %></td>
        </tr>
				<% end %>
      </tbody>
    </table>
	</div>
