<%= provide(:title, "Exchange") %>
<div class="main">
	<div class="section" style="padding-top: 60px;">
		<div class="container">
			<div class="row">
				<div class="col-md-12 mb-4">
					<h1 class="h2"><%= @exchange.name %></h1>
					
				  
					<ul class="list-unstyled">
					<% @currencies.each do |currency| %>
						<li><%= link_to currency.name, currency_path(currency) %></li>
					<% end %>
				</ul>
				</div>
			</div>
		</div>
	</div>
</div>


